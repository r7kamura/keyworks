{
  "name": "Keyworks",
  "description": "__MSG_applicationDescription__",
  "version": "0.0.1",
  "author": "Ryo Nakamura",
  "background": {
    "scripts": [
      "javascripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "javascripts/content.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en",
  "homepage_url": "https://github.com/r7kamura/keyworks",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "clipboardRead",
    "https://*/*",
    "http://*/*"
  ],
  "short_name": "Keyworks"
}
